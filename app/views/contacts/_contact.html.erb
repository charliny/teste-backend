<div id="<%= dom_id contact %>">
  <p>
    <strong>Name:</strong>
    <%= contact.name %>
  </p>

  <p>
    <strong>CPF:</strong>
    <%= contact.cpf %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>Mobile:</strong>
    <%= contact.mobile %>
  </p>

  <p>
    <strong>Birthday:</strong>
    <%= contact.birthday %>
  </p>

  <p>
    <strong>Presence:</strong>
    <%= contact.presence %>
  </p>

</div>
